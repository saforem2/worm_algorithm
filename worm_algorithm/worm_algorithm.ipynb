    "from scipy.interpolate import UnivariateSpline\n",
    "def fwhm1(x, y):\n",
    "    # create a spline of x and blue-np.max(blue)/2 \n",
    "    spline = UnivariateSpline(x, y-np.max(y)/2, s=0)\n",
    "    roots = spline.roots()\n",
    "    r1 = roots[0]\n",
    "    r2 = roots[-1]\n",
    "    #r1, r2 = spline.roots() # find the roots\n",
    "    return np.abs(r1 - r2)"
